<p id="notice"><%= notice %></p>

<h1>Listing Reiteos</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reiteos.each do |reiteo| %>
      <tr>
        <td><%= reiteo.description %></td>
        <td><%= link_to 'Show', reiteo %></td>
        <td><%= link_to 'Edit', edit_reiteo_path(reiteo) %></td>
        <td><%= link_to 'Destroy', reiteo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reiteo', new_reiteo_path %>
